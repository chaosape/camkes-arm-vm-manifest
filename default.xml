<manifest>
  <remote fetch="../seL4" name="seL4"/>
  <remote fetch="../sel4proj" name="sel4proj"/>
  <remote fetch="../chaosape" name="chaosape"/>
  <remote fetch="." name="smaccm"/>  
  <remote fetch="../afrl-rq" name ="afrl-rq"/>
  
  <default remote="seL4" revision="master"/>
  
  <project
      name="DARPA-bsp.git"
      path="projects/DARPA-tk1"
      remote="smaccm"
      upstream="master"/>
  
  <project
      name="camkes-arm-vm.git"
      path="projects/vm"
      remote="chaosape"
      revision="WayPointManager"
      upstream="master"/>
  
  <project name="camkes-tool.git" path="tools/camkes" upstream="master">
    <linkfile dest="CMakeLists.txt" src="camkes-top-level.cmake"/>
    <linkfile dest="camkes_README.md" src="docs/index.md"/>
  </project>

  <project name="capdl.git" path="projects/capdl" upstream="master"/>

  <project
      name="global-components.git"
      path="projects/global-components"
      remote="sel4proj"
      upstream="master"/>

  <project
      name="musllibc.git"
      path="projects/musllibc"
      upstream="sel4"/>

  <project
      name="projects_libs.git"
      path="projects/projects_libs"
      remote="sel4proj"
      upstream="master"/>

  <project name="seL4.git" path="kernel" upstream="master"/>

  <project name="seL4_libs.git" path="projects/seL4_libs" upstream="master"/>

  <project
      name="seL4_projects_libs.git"
      path="projects/seL4_projects_libs"
      remote="sel4proj"
      upstream="master"/>

  <project name="seL4_tools.git" path="projects/seL4_tools" upstream="master">
    <linkfile dest="init-build.sh" src="cmake-tool/init-build.sh"/>
  </project>

  <project name="util_libs.git" path="projects/util_libs" upstream="master"/>

  <project name="clmcp" path="clmcp" remote="chaosape" upstream="master"/>

  <project
      name="OpenUxAS.git"
      path="OpenUxAS"
      remote="afrl-rq"
      revision="master"
      upstream="afrl-rq"/>
    
</manifest>
